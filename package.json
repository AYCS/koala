{
  "name": "koala",
  "description": "Koa Framework Suite",
  "version": "1.0.0",
  "author": {
    "name": "Jonathan Ong",
    "email": "me@jongleberry.com",
    "url": "http://jongleberry.com",
    "twitter": "https://twitter.com/jongleberry"
  },
  "license": "MIT",
  "repository": "koajs/koala",
  "dependencies": {
    "basic-auth": "1",
    "bluebird": "3.5.0",
    "commander": "2",
    "debug": "*",
    "delegates": "1.0.0",
    "koa": "1.4.0",
    "koa-body-parsers": "^1.1.0",
    "koa-cash": "2",
    "koa-compress": "1",
    "koa-csrf": "2",
    "koa-error": "2",
    "koa-etag": "2",
    "koa-file-server": "2",
    "koa-json": "1",
    "koa-logger": "1",
    "koa-qs": "^2.0.0",
    "koa-response-time": "1",
    "koa-safe-jsonp": "0",
    "koa-session": "^4.5.0",
    "koa-trace": "1",
    "merge-descriptors": "^1.0.0",
    "ms": "2.0.0",
    "mz": "^2.0.0",
    "qs": ">= 2",
    "snyk": "^1.40.2"
  },
  "devDependencies": {
    "istanbul": "^0.4.5",
    "mocha": "3",
    "supertest": "^2.0.0"
  },
  "main": "lib",
  "files": [
    "lib",
    "bin",
    "docs",
    "template"
  ],
  "scripts": {
    "test": "NODE_ENV=test mocha --reporter spec --timeout 30s --bail test/app/index.js",
    "test-cov": "NODE_ENV=test node node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --reporter dot --timeout 2m --bail test/app/index.js",
    "test-travis": "NODE_ENV=test node node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- --reporter dot --timeout 2m --bail test/app/index.js",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "koa",
    "http",
    "https",
    "http2",
    "framework",
    "generators",
    "es6",
    "modules",
    "promises",
    "push"
  ],
  "snyk": true
}
